{
    "manifest_version": 3,
    "name": "Day Wrapped Extension",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    }
  }